
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'pci0000:00/0000:00:01.0/0000:02:00.0'
	option htmode 'HT20'
	option channel 'auto'
	option country 'PH'
	option txpower '20'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option mode 'ap'
	option isolate '0'
	option ssid 'LODI Wi-Fi 2020'
	option network 'wlan1'
	option key 'newnormalnetwork'
	option encryption 'none'

config wifi-iface 'eap_radio0'
	option ssid 'LODI Wi-Fi 2020 Secure'
	option encryption 'wpa2'
	option device 'radio0'
	option mode 'ap'
	option auth_secret 'newnormalnetwork'
	option acct_secret 'newnormalnetwork'
	option auth_server '192.168.200.254'
	option acct_server '192.168.200.254'
	option nasid 'newhotspot'
	option auth_port '1812'
	option acct_port '1813'
	option isolate '0'
	option network 'wlan2'

config wifi-device 'radio1'
	option type 'mac80211'
	option hwmode '11a'
	option path 'pci0000:00/0000:00:00.0/0000:01:00.0'
	option channel 'auto'
	option country 'PH'
	option txpower '17'
	option legacy_rates '0'
	option htmode 'VHT80'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option mode 'ap'
	option isolate '0'
	option ssid 'LODI Wi-Fi 2020'
	option network 'wlan1'
	option key 'newnormalnetwork'
	option encryption 'none'


config wifi-iface 'eap_radio1'
	option ssid 'LODI Wi-Fi 2020 Secure'
	option encryption 'wpa2'
	option device 'radio1'
	option mode 'ap'
	option auth_secret 'newnormalnetwork'
	option acct_secret 'newnormalnetwork'
	option nasid 'newhotspot'
	option auth_server '192.168.200.254'
	option acct_server '192.168.200.254'
	option auth_port '1812'
	option acct_port '1813'
	option isolate '0'
	option network 'wlan2'

